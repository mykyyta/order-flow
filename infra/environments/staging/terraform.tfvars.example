project_id      = "YOUR_GCP_PROJECT_ID"
region          = "us-central1"
environment     = "staging"
secret_id_prefix = "pult-staging"
service_name    = "pult-staging-app"
migrate_job_name = "pult-staging-migrate"
artifact_repository_id = "my-repo"
container_image        = "us-central1-docker.pkg.dev/YOUR_GCP_PROJECT_ID/my-repo/my-app"
service_account_email  = "YOUR_PROJECT_NUMBER-compute@developer.gserviceaccount.com"
container_cpu_limit    = "1000m"
container_memory_limit = "512Mi"
container_concurrency  = 80
max_instances          = 100
public_invoker_member  = "allUsers"

# Optional: custom domain for staging Cloud Run (must be verified first). Leave empty to skip.
custom_domain = ""

migrate_job_timeout    = 1200
enable_migrate_job     = false
manage_secret_versions = false
manage_shared_ci_resources = false
enable_telegram_bot_token_secret = false

github_repository      = "your-username/pult"
wif_pool_id            = "github-actions-pool"
wif_provider_id        = "github-actions-provider"
terraform_deployer_sa_id = "your-tf-deployer-sa"
tf_state_bucket_name   = "YOUR_GCP_PROJECT_ID-tfstate"

# For one-time bootstrap you can set manage_secret_versions=true
# and provide values locally (never commit real secrets):
#
# secret_values = {
#   django_secret_key  = "..."
#   database_url       = "..."
#   telegram_bot_token = "..."
# }
